{
  "name": "baby-gallery",
  "version": "0.0.1",
  "description": "tool to manage baby gallery",
  "main": "./src/main.js",
  "scripts": {
    "start": "NODE_ENV=debug ELECTRON_ENABLE_LOGGING=true electron .",
    "postinstall": "electron-builder install-app-deps",
    "release": "NODE_ENV=release electron-builder"
  },
  "repository": "https://github.com/zxyqwe/baby-gallery",
  "keywords": [
    "Electron",
    "zxyqwe",
    "baby-gallery"
  ],
  "build": {
    "productName": "baby-gallery",
    "appId": "com.zxyqwe.baby.gallery",
    "directories": {
      "output": "build"
    },
    "files": [
      "src/**/*"
    ],
    "mac": {
      "target": "zip"
    },
    "win": {
      "target": "portable"
    },
    "linux": {
      "target": "zip"
    }
  },
  "author": "zxyqwe",
  "license": "CC0-1.0",
  "dependencies": {
    "electron-log": "~3.0.5",
    "axios": "~0.18.0",
    "fs-jetpack": "~2.2.2"
  },
  "devDependencies": {
    "axios": "~0.18.0",
    "fs-jetpack": "~2.2.2",
    "electron": "~4.1.4",
    "electron-builder": "~20.39.0",
    "electron-cookies": "~1.1.0",
    "electron-log": "~3.0.5",
    "fs-extra": "~4.0.2",
    "lowdb": "~1.0.0",
    "vue": "~2.3.4"
  }
}
